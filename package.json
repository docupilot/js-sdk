{
  "name": "@docupilot/docupilot-js-sdk",
  "version": "1.0.0",
  "description": "interact with docupilot.app APIs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "ts-node examples/basic.ts",
    "generate.app-spec.node": "node generate.js --input ./openapi-app.yaml --output ./src/api --useOptions --useUnionTypes --client node",
    "generate.app-spec.react": "node generate.js --input ./openapi-app.yaml --output ./src/api --useOptions --useUnionTypes --client xhr",
    "generate.app-spec.browser": "node generate.js --input ./openapi-app.yaml --output ./src/api --useUnionTypes --client xhr",
    "generate.accounts-spec.node": "node generate.js --input ./openapi-accounts.yaml --output ./src/accounts --useOptions --useUnionTypes --client node",
    "generate.accounts-spec.react": "node generate.js --input ./openapi-accounts.yaml --output ./src/accounts --useOptions --useUnionTypes --client xhr",
    "generate.accounts-spec.browser": "node generate.js --input ./openapi-accounts.yaml --output ./src/accounts --useUnionTypes --client xhr",
    "build": "npm run clean:dist && npm run build:web && npm run build:esm && npm run build:cjs",
    "clean:dist": "rm -rf dist/",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:web": "npm run generate.app-spec.browser && npm run generate.accounts-spec.browser && npx webpack --mode=development && npx webpack --mode=production && npm run generate.app-spec.react && npm run generate.accounts-spec.react"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docupilot/docupilot-js-sdk"
  },
  "publishConfig": {
    "@docupilot:registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "docupilot.app",
    "docupilot apis",
    "doc gen apis",
    "generate word document",
    "generate pdf",
    "generate powerpoint",
    "generate excel",
    "fill pdf forms"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@docupilot/openapi-typescript-codegen": "^0.23.8",
    "@types/node-fetch": "^2.6.2",
    "braces": "^3.0.3",
    "form-data": "^4.0.4",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "yaml": "^2.5.1"
  }
}
